#include <stdio.h>

int main() {
    int nums[] = {2, 2, 1, 1, 1, 2, 2, 3}; // Example input
    int n = sizeof(nums) / sizeof(nums[0]);
    int count, majority = -1;

    for (int i = 0; i < n; i++) {
        count = 0;
        for (int j = 0; j < n; j++) {
            if (nums[j] == nums[i]) {
                count++;
            }
        }
        if (count > n / 2) {
            majority = nums[i];
            break;
        }
    }

    printf("%d\n", majority);
    return 0;
}
