#include <stdio.h>

int main() {
    int arr[] = {1, 4, 2, 10, 23, 3, 1, 0, 20}; // Example input
    int k = 4;
    int n = sizeof(arr) / sizeof(arr[0]);

    if (k > n || k <= 0) {
        printf("Invalid window size\n");
        return 0;
    }

    int maxSum = 0;

    // Compute sum of first window
    for (int i = 0; i < k; i++) {
        maxSum += arr[i];
    }

    int windowSum = maxSum;

    // Slide the window
    for (int i = k; i < n; i++) {
        windowSum += arr[i] - arr[i - k];
        if (windowSum > maxSum) {
            maxSum = windowSum;
        }
    }

    printf("%d\n", maxSum);
    return 0;
}
