#include <stdio.h>

int main() {
    int n, target;

    // Read array size
    printf("Enter size of array: ");
    scanf("%d", &n);

    int nums[n];

    // Read array elements
    printf("Enter %d positive integers:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }

    // Read target value
    printf("Enter target sum: ");
    scanf("%d", &target);

    // Brute-force search for two indices
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (nums[i] + nums[j] == target) {
                printf("%d %d\n", i, j);
                return 0;
            }
        }
    }

    // If no pair found
    printf("-1 -1\n");
    return 0;
}
