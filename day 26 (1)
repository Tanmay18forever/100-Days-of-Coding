#include <stdio.h>

int main() {
    int i, j;

    // Outer loop for rows, from 5 down to 1
    for (i = 5; i >= 1; i--) {
        // Inner loop for printing numbers in each row
        // Starts from the current row value (i) and goes up to 5
        for (j = i; j <= 5; j++) {
            printf("%d", j);
        }
        printf("\n"); // New line after each row
    }

    return 0;
}
