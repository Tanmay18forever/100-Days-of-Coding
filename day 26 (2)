#include <stdio.h>

int main() {
   
    int max_group_size = 5;

    
    for (int i = 1; i <= max_group_size; i = i + 2) {
        
        for (int j = 1; j <= i; j++) {
            printf("*\n");
        }
        
       
        printf("\n"); 
    }


    for (int i = max_group_size - 2; i >= 1; i = i - 2) {
      
        for (int j = 1; j <= i; j++) {
            printf("*\n");
        }
        
        
        printf("\n"); 
    }

    return 0;
}
